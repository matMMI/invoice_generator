{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd frontend && pnpm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && pnpm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/api/auth/:path*"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  }
}
